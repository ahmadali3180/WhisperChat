  pod 'FirebaseAuth'
  pod 'FirebaseDatabase'
  pod 'FirebaseCore'
  pod 'FirebaseFirestore'
  
  #  Facebook
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  
end
