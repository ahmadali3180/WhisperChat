# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

use_frameworks!

target 'WhisperChat' do
  
  #  Firebase
  pod 'FirebaseAuth'
  pod 'FirebaseDatabase'
  pod 'FirebaseCore'
  pod 'FirebaseFirestore'
  
  #  Facebook
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  
  #  Google SignIn
  pod 'GoogleSignIn'

end
